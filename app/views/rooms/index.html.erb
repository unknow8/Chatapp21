<div class="container-fluid">
  <div class="row">
    <div class="col-3">
        <%= render 'form', room: Room.new %>
        <hr>
        <% @rooms.each do |room| %>
          <%= link_to room do %>
          <div class="card">
            <%= room.name %>
          </div>
          <% end %>
        <% end %>
    </div>
  </div>
</div>
